extends layout
  
block content
  link(rel='stylesheet', href='/stylesheets/friends.css') 
  .friends
    each friend in friends
      .box
        a(href= friend.page)
          img.friendAvatar(src= friend.avatar)
          p.friendName.tc= friend.name 
  .menu
    .img.menuIMG
    p.menuName.tc= 'ИМЯ'
    a(href='/seach')
      img.menuIMG#seach  
    .img.menuIMG
    a(href='/settings')
      .img.menuIMG(hidden="true")      
    a(href='/')
      p.bottom.tc= 'ВЫХОД'      